import sys
import json
from generator import generator

class Driver(process):

    def setup():
        pass

    def run():

        with open('./config.json') as f:
            data = json.load(f)

            num_nodes = data['num_nodes']
            byzentine_nodes = data['byzentine_nodes']
            rounds = data['rounds']
            limit_test_cases = data['limit_test_cases']
            output_file_name = data['output_file_name']

            nodes = [i for i in range(num_nodes)]

            gen = generator.TestGenerator(nodes, rounds, limit_test_cases)

            # generates testcases
            gen.run(True)




def main():

    driver = new(Driver)

    setup(driver, ())

    start(driver)

